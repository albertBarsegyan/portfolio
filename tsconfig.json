{

  "compilerOptions": {
    // "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "esModuleInterop": true,
    "strictNullChecks": true
  },


  "rules": {
    "quotemark": [
      true,
      "single"
    ]
  },
  "include": ["src/components/typescript/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]


}